{
  "name": "Commute Time",
  "version": "0.0.1",
  "description": "Get commute time on zillow, craigslist + more to come",
  "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://www.zillow.com/homes/*"],
      "js" : ["jquery.js", "showscript.js"]

    }
  ],
  "manifest_version": 2,
  "permissions":["storage", "http://www.mapquestapi.com/*"]

}
